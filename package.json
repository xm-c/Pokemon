{
  "name": "pokemon",
  "version": "1.0.0",
  "private": true,
  "description": "一款宝可梦检索软件",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Less",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "check-node": "node -e \"if (process.versions.node.split('.')[0] < 20) { console.error('\\nError: This project requires Node.js 20 or higher. Current version: ' + process.versions.node); process.exit(1); }\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --quiet",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "new": "taro new",
    "patch": "weapp-tw patch",
    "build:weapp": "npm run patch && taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "npm run check-node && taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run patch && taro build --type weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run check-node && npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "postinstall": "npm run patch"
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@tarojs/components": "4.1.4",
    "@tarojs/components-advanced": "^4.1.4",
    "@tarojs/helper": "4.1.4",
    "@tarojs/plugin-framework-react": "4.1.4",
    "@tarojs/plugin-platform-alipay": "4.1.4",
    "@tarojs/plugin-platform-h5": "4.1.4",
    "@tarojs/plugin-platform-harmony-hybrid": "4.1.4",
    "@tarojs/plugin-platform-jd": "4.1.4",
    "@tarojs/plugin-platform-qq": "4.1.4",
    "@tarojs/plugin-platform-swan": "4.1.4",
    "@tarojs/plugin-platform-tt": "4.1.4",
    "@tarojs/plugin-platform-weapp": "4.1.4",
    "@tarojs/react": "4.1.4",
    "@tarojs/runtime": "4.1.4",
    "@tarojs/shared": "4.1.4",
    "@tarojs/taro": "4.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tarojs/cli": "4.1.4",
    "@tarojs/plugin-generator": "4.1.4",
    "@tarojs/vite-runner": "4.1.4",
    "@types/minimatch": "^5.1.2",
    "@types/react": "^18.0.0",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.21",
    "babel-preset-taro": "4.1.4",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "react-refresh": "^0.14.0",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.3.5",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "vite": "^4.2.0",
    "vite-plugin-cjs-interop": "^2.2.0",
    "weapp-tailwindcss": "^4.2.0-alpha.2"
  }
}
